train dqn agent:

python run.py --mode train --env_config data/envs/atari_pong.yaml --agent_config a3/atari_dqn_agent.yaml --log_file output/dqn_log.txt --out_model_file output/dqn_model.pt --max_samples 3000000 --visualize

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

test dqn agent:

python run.py --mode test --env_config data/envs/atari_pong.yaml --agent_config a3/atari_dqn_agent.yaml --model_file output/dqn_model.pt --visualize

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
